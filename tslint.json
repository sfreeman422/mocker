{
  "extends": [
    "tslint:latest",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-implicit-dependencies": [true, "dev"]
  },
  "linterOptions": {
    "exclude": ["./*.json", "./*.md"]
  }
}
